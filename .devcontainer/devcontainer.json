{
  "name": "Haikus for june",
  "workspaceFolder": "/workspace",


//container image to be assembled 
  "dockerComposeFile": "docker-compose.yml",
  "service": "web",

  "remoteUser": "node",

  "runServices": [],
//commands automatically run after codespace creation
   "postCreateCommand": "npm install",

//vscode extensions for the team 
  "extensions": [
	"esbenp.prettier-vscode",
	"MS-vsliveshare.vsliveshare"
],

//ports to be forwarded from inside the container to the local machine
  "forwardPorts": [3000]
}
